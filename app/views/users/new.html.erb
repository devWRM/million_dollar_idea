<h1>The Signup Page</h1>

<%= render partial: "layouts/errors", locals: {class_object: @user} %>

<%= form_for(@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
        </br></br>
    <%= f.label :email %>
    <%= f.email_field :email %>
        </br></br>
    <%= f.label :password %>
    <%= f.password_field :password %>
        </br></br>

    <%= f.submit "Submit" %>
<% end %>



